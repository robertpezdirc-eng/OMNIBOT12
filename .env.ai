# OMNI CORE AI - Konfiguracija
# Kopiraj to datoteko v .env.ai.local in dodaj svoje API kljuƒçe

# OpenAI API konfiguracija
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_MAX_TOKENS=150
OPENAI_TEMPERATURE=0.1

# AI Routing nastavitve
AI_ROUTING_ENABLED=true
AI_FALLBACK_ENABLED=true
AI_CONFIDENCE_THRESHOLD=0.3

# Logging
AI_LOG_LEVEL=INFO
AI_LOG_REQUESTS=true

# Performance
AI_CACHE_ENABLED=true
AI_CACHE_TTL=300
AI_TIMEOUT_SECONDS=10

# Backup routing
FALLBACK_TO_KEYWORDS=true
KEYWORD_CONFIDENCE_BOOST=0.2

# Monitoring
TRACK_AI_PERFORMANCE=true
SAVE_AI_RESPONSES=false