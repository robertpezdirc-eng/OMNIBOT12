{
  "name": "omni-admin-gui",
  "version": "1.0.0",
  "description": "Omni License System - Admin GUI za upravljanje licenc",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "pack": "electron-builder --dir"
  },
  "keywords": [
    "omni",
    "license",
    "admin",
    "electron",
    "gui"
  ],
  "author": "Omni System",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "electron": "^28.0.0",
    "socket.io-client": "^4.7.4",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.omni.admin",
    "productName": "Omni Admin",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!node_modules/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    }
  }
}