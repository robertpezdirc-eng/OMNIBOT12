{
  "name": "omni-admin-gui",
  "version": "1.0.0",
  "description": "Omni Admin Panel - Skrbni≈°ka konzola za upravljanje licenc",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "electron",
    "admin",
    "license",
    "management"
  ],
  "author": "Omni AI Platform",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "socket.io-client": "^4.8.1"
  },
  "build": {
    "appId": "com.omni.admin-gui",
    "productName": "Omni Admin Panel",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "renderer.js",
      "index.html",
      "node_modules/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    }
  }
}
