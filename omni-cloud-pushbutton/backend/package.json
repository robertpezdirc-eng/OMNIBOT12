{
  "name": "omni-backend",
  "version": "1.0.0",
  "description": "Omni Cloud Backend - Push-button ready deployment with license management",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test.js",
    "test:watch": "nodemon test.js",
    "test:full": "node ../test-runner.js --full",
    "test:quick": "node ../test-runner.js --quick",
    "test:debug": "node ../test-runner.js --debug",
    "debug": "DEBUG_MODE=true LOG_LEVEL=debug node server.js",
    "health": "node -e \"require('./debug.js').getHealthStatus()\"",
    "report": "node -e \"require('./debug.js').generateDebugReport()\""
  },
  "keywords": [
    "omni",
    "cloud",
    "backend",
    "license",
    "websocket",
    "express",
    "mongodb"
  ],
  "author": "Omni Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "winston": "^3.10.0",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "nodemailer": "^6.9.4",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "axios": "^1.5.0",
    "socket.io-client": "^4.7.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}